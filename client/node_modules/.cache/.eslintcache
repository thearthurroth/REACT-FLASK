password: \"pass2\"\n        }\n    ];\n\n    const errors = {\n        uname: \"invalid username\",\n        pass: \"invalid password\"\n    };\n\n\n    const handleSubmit = (event) => {\n        //Prevent page reload\n        event.preventDefault();\n\n        var { uname, pass } = document.forms[0];\n\n        // Find user login info\n        const userData = database.find((user) => user.username === uname.value);\n\n        // Compare user info\n        if (userData) {\n            if (userData.password !== pass.value) {\n                // Invalid password\n                setErrorMessages({ name: \"pass\", message: errors.pass });\n            } else {\n                setIsSubmitted(true);\n                if (uname.value === \"user1\"){\n                let link = \"/dashboard/\"+uname.value\n                navigate(link)} else if(uname.value === \"admin2\") {\n                    let link = \"/admin/dashboard/\"+uname.value\n                    navigate(link)\n                }\n                \n                \n            }\n        } else {\n            // Username not found\n            setErrorMessages({ name: \"uname\", message: errors.uname });\n        }\n\n\n        \n            \n    };\n\n    // Generate JSX code for error message\n    const renderErrorMessage = (name) =>\n        name === errorMessages.name && (\n            <div className=\"error\">{errorMessages.message}</div>\n        );\n\n    // JSX code for login form\n    const renderForm = (\n        <div className=\"form\">\n            <form onSubmit={handleSubmit}>\n                <div className=\"input-container\">\n                    <label>Username </label>\n                    <input type=\"text\" name=\"uname\" required />\n                    {renderErrorMessage(\"uname\")}\n                </div>\n                <div className=\"input-container\">\n                    <label>Password </label>\n                    <input type=\"password\" name=\"pass\" required />\n                    {renderErrorMessage(\"pass\")}\n                </div>\n                <div className=\"button-container\">\n                    <input type=\"submit\" />\n                </div>\n            </form>\n        </div>\n    );\n\n    return (\n        <div className=\"login-form\">\n            <div className=\"title\">Sign In</div>\n            {isSubmitted ? <div>User is successfully logged in</div>   : renderForm}\n\n        </div>\n    );\n}\nexport default LoginMask;","/Users/arthurroth/Desktop/REACT-FLASK/client/src/pages/Login.js",["46"],[],"import React from 'react'\nimport LoginMask from '../components/LoginMask'\nimport './Login.css'\nimport Container from '@material-ui/core/Container';\n\nconst Login = () => {\n  return (\n    <div className='Login'>\n        <LoginMask/>\n    </div>\n  )\n}\n\nexport default Login",{"ruleId":"47","severity":1,"message":"48","line":1,"column":16,"nodeType":"49","messageId":"50","endLine":1,"endColumn":24},{"ruleId":"47","severity":1,"message":"51","line":1,"column":26,"nodeType":"49","messageId":"50","endLine":1,"endColumn":35},{"ruleId":"47","severity":1,"message":"52","line":2,"column":10,"nodeType":"49","messageId":"50","endLine":2,"endColumn":14},{"ruleId":"47","severity":1,"message":"53","line":2,"column":31,"nodeType":"49","messageId":"50","endLine":2,"endColumn":40},{"ruleId":"54","replacedBy":"55"},{"ruleId":"56","replacedBy":"57"},{"ruleId":"58","severity":1,"message":"59","line":7,"column":3,"nodeType":"60","messageId":"61","endLine":7,"endColumn":45},{"ruleId":"47","severity":1,"message":"51","line":1,"column":17,"nodeType":"49","messageId":"50","endLine":1,"endColumn":26},{"ruleId":"47","severity":1,"message":"62","line":11,"column":13,"nodeType":"49","messageId":"50","endLine":11,"endColumn":25},{"ruleId":"47","severity":1,"message":"63","line":4,"column":8,"nodeType":"49","messageId":"50","endLine":4,"endColumn":17},"no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar"